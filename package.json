{
  "name": "domo-sdk",
  "version": "2.0.1",
  "description": "Javascript Domo SDK",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts --project ./tsconfig.json --type-check",
    "test": "mocha --opts mocha.opts",
    "tdd": "mocha --opts mocha.opts --watch",
    "build": "rm -rf ./dist && tsc",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --opts .mocharc",
    "upload": "npm run test && npm version && npm run build && npm publish",
    "clean": "rm -rf node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domoinc/domo-node-sdk"
  },
  "keywords": [
    "Domo"
  ],
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.6",
    "growl": "^1.10.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/debug": "0.0.29",
    "@types/mocha": "^2.2.41",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.8",
    "@types/request": "0.0.45",
    "@types/request-promise-native": "^1.0.5",
    "@types/sinon": "^2.3.2",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "nock": "^9.0.13",
    "sinon": "^2.3.6",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.2.1",
    "tsutils": "^2.5.1",
    "typescript": "^2.4.1"
  },
  "engines": {
    "node": ">=6.4.0"
  }
}
