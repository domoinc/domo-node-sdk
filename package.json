{
  "name": "@neatcapital/domo-sdk",
  "version": "2.1.1",
  "description": "Javascript Domo SDK",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts --project ./tsconfig.json --type-check",
    "test": "mocha --opts mocha.opts",
    "tdd": "mocha --opts mocha.opts --watch",
    "build": "rm -rf ./dist && tsc",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --opts .mocharc",
    "upload": "npm run test && npm version && npm run build && npm publish",
    "clean": "rm -rf node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domoinc/domo-node-sdk"
  },
  "keywords": [
    "Domo"
  ],
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.1",
    "growl": "^1.10.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/debug": "4.1.5",
    "@types/mocha": "^8.2.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^14.14.33",
    "@types/request": "2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^9.0.11",
    "chai": "^4.3.3",
    "istanbul": "^0.4.5",
    "mocha": "^8.3.1",
    "nock": "^13.0.11",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tsutils": "^3.21.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=6.4.0"
  }
}
